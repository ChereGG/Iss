<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Inchiriere"
           table="Inchirieri">
        <id name="id" type="long" column="id">
            <generator class="increment"/>
        </id>
        <property
                name="idCarte" type="long" column="idCarte"
        />
        <property
                name="idUtilizator" type="long" column="idUtilizator"
        />
        <property
                name="returned" type="boolean" column="returned"
        />
    </class>
</hibernate-mapping>
